{
  "name": "spritefont",
  "author": {
    "name": "Andor Polgar",
    "email": "hngrhorace@gmail.com",
    "url": "http://andor.cool"
  },
  "license": "MIT",
  "keywords": [
    "sprite",
    "generator",
    "font",
    "letter"
  ],
  "version": "0.1.2",
  "description": "Renders sprite fonts",
  "main": "dist/index.js",
  "scripts": {
    "build:babel": "babel src --out-dir dist",
    "build:browserify": "mkdir -p browser && browserify dist/index.js --standalone SpriteFont > browser/spritefont.js",
    "build": "npm run build:babel && npm run build:browserify",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:andormade/spritefont.git"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^13.0.0"
  },
  "dependencies": {
    "core-js": "^2.4.1"
  }
}
